<table class="table table-hover parsing_filter-table">
  <tr>
    <th>is_work</th>
    <th>keyword</th>
    <th>type</th>
    <th></th>
  </tr>
  <% for parsing_filter in @parsing_filters %>
    <tr>
      <td><%= parsing_filter.is_work_str %></td>
      <td><%= parsing_filter.keyword %></td>
      <td><%= parsing_filter.keyword_type_str %></td>
      <td><%= link_to "Edit", edit_admin_parsing_filter_path(parsing_filter), class: "btn btn-warning" %>
      <%= link_to "Destroy", admin_parsing_filter_path(parsing_filter, page: params[:page]), :method => :delete, class: "btn btn-danger delete_post" %></td>
    </tr>
  <% end %>
</table>
<%= paginate @parsing_filters %>

