<div class="row">
  <!-- Servers monitored -->
  <div class="col-lg-3 col-md-6">
    <div class="panel panel-orange-rust">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-tv fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= FleaMarket.searchable.count %></div>
            <div>Live Fleamarket</div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Servers up -->
  <div class="col-lg-3 col-md-6">
    <div class="panel panel-green-rust">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-arrow-up fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge">97 %</div>
            <div>Average uptime</div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Players online -->
  <div class="col-lg-3 col-md-6">
    <div class="panel panel-blue-rust">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-line-chart fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge">17.6 k</div>
            <div>Players online</div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Servers down -->
  <div class="col-lg-3 col-md-6">
    <div class="panel panel-red-rust panel-serversDown" style="cursor: pointer;">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-arrow-down fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= FleaMarket.where(is_visible: 2).count %></div>
            <div>Waiting Count</div>  
          </div>
        </div>
      </div>
    </div>
  </div>

  </div>
<div class="search_form">
   <%= form_tag admin_flea_markets_path, remote: true, class: "form-inline",  :method => :get  do %>
     <div class="form-group">
       <%= label_tag  :name, "마켓명"  %>
       <%= text_field_tag :name, params[:name], :class => 'form-control'  %>
       <%= check_box_tag :pending, "1", params[:pending], :class => 'form-control'  %> 심사만
     </div>

     <%= submit_tag "Search", class: "btn btn-warning" %>
   <% end %>
</div>



<p><%= link_to "New Admin/Flea Market", new_admin_flea_market_path, class: "btn btn-primary" %></p>


<table class="table table-hove">
  <tr>
    <th>Name</th>
    <th>Visible</th>
    <th>DateRange</th>
    <th>Img</th>
    <th></th>
  </tr>
  <% for flea_market in @flea_markets %>
    <tr>
      <td><%= flea_market.market_name %></td>
      <td><%= flea_market.is_visible_str%></td>
      <td><%= flea_market.start_date.strftime("%Y-%m-%d %H:%M") if flea_market.start_date.present? %> ~ <%= flea_market.end_date.strftime("%Y-%m-%d %H:%M") if flea_market.end_date.present? %></td>
      <td><%= image_tag flea_market.top_image_url, size: '200x200' if flea_market.top_image.present?%></td>
      <td><%= link_to "Edit", edit_admin_flea_market_path(flea_market), class: "btn btn-warning" %>
      <%= link_to "Destroy", [:admin, flea_market], :confirm => 'Are you sure?', :method => :delete, class: "btn btn-danger" %></td>
    </tr>
  <% end %>
</table>

<%= paginate @flea_markets %>

<p><%= link_to "New Admin/Flea Market", new_admin_flea_market_path, class: "btn btn-primary" %></p>


