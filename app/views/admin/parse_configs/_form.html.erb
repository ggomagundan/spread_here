<%= form_for [:admin, @parse_config], html: {class: "form-horizontal" } do |f| %>

  <div class="form-group">
    <label class="control-label col-sm-2">cookie</label>
    <div class="controls col-sm-11">
      <%= f.text_area :cookie, class: "form-control", rows: 20   %>
    </div>
  </div>


  <div class="form-actions">
    <%= f.submit "Update", :class => "btn btn-success" %>
  </div>

<% end %>
